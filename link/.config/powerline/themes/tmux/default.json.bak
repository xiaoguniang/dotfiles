{
	"segments": {
		"right": [
			{
			  "function": "powerline.segments.common.time.date",
			  "name": "time",
			  "args": {
				"format": "%H:%M",
				"istime": true
			  }
			},
			{
				"function": "powerline.segments.common.time.date",
				"args": {
				  "format": "%m-%d",
				  "istime": true
				}
			},
			{
			  "function": "powerline.segments.common.sys.system_load",
			  "priority": 50
			},
			{
			  "function": "powerline.segments.common.net.network_load",
			  "name": "network",
			  "args": {
				"si_prefix": true,
				"suffix": "B/s ",
				"recv_format": "⬇{value:>9}",
				"sent_format": "⬆{value:>9}"
			  }
			}
		]
	}
}
