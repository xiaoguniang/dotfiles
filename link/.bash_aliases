alias bc='bc -lq'
alias glogout='gnome-session-quit'
alias wine='env LANG=zh LANG=zh_CN.utf8 wine'
alias lqq-sync='lqq && rsync -ar * lqq:~/tool/lhb'
alias getbind='bind -P | grep -v "not bound"'

# shortcut for path
alias leet='cd ~/projects/leetcode'
alias topcoder='cd ~/projects/topcoder'
alias ncic='cd ~/projects/ncic/'
alias lqq='ncic && cd lqq'
